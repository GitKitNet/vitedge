{
  "name": "example",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vitedge --force",
    "dev:ssr": "vitedge --ssr --force",
    "build": "rm -rf dist && vitedge build",
    "refresh": "rm -rf node_modules/vitedge && cp -r ../../src node_modules/vitedge && cp ../../src/bin/cli.js node_modules/.bin/vitedge",
    "serve:worker": "cd ../worker-site && yarn serve",
    "serve:node": "cd ../node-server && yarn serve"
  },
  "dependencies": {
    "@vueuse/head": "^0.3.1",
    "vite": "^2.0.0",
    "vitedge": "file:../../src",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}